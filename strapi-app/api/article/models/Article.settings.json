{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "headline": {
      "type": "string"
    },
    "kicker": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "status": {
      "default": "draft",
      "enum": [
        "published",
        "draft",
        "deleted"
      ],
      "type": "enumeration"
    },
    "poster": {
      "type": "json"
    },
    "content": {
      "type": "json"
    },
    "colorBackground": {
      "type": "string"
    },
    "colorContrast": {
      "type": "string"
    },
    "colorText": {
      "type": "string"
    },
    "topic": {
      "model": "topic",
      "via": "articles"
    },
    "author": {
      "collection": "user",
      "dominant": true,
      "via": "articles",
      "plugin": "users-permissions"
    },
    "collections": {
      "collection": "collection",
      "via": "articles"
    }
  }
}